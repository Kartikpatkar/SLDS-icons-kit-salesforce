{
    "name": "Icons Kit for Salesforce Developer",
    "description": "Browse, preview, customize, and copy Salesforce SLDS icons.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Icons Kit for Salesforce Developer",
        "default_icon": {
            "128": "assets/extension-icons/logoIcon.png"
        }
    },
    "icons": {
        "128": "assets/extension-icons/logoIcon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [],
    "host_permissions": [],
    "web_accessible_resources": [{
        "resources": ["assets/icons/**/*", "icons-index.json"],
        "matches": ["<all_urls>"]
    }]
}